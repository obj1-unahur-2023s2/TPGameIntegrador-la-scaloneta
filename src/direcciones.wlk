object arriba {
	method proximaPosicion(unaPosicion) = unaPosicion.up(1)
}

object derecha {
	method proximaPosicion(unaPosicion) = unaPosicion.right(1) 
}

object izquierda{
	method proximaPosicion(unaPosicion) = unaPosicion.left(1)
}

object abajo{
	method proximaPosicion(unaPosicion) = unaPosicion.down(1)
}
